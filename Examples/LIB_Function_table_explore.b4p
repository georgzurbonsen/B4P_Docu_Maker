#!/usr/local/bin/b4p
       table load( test, "Table Explore Demo.csv" );
       table list ( test );
       coords[] = table explore ( test );

       echo("Identified ", coords[]{}," sub-tables:");
       for all ( coords[], c[], i[] )
       {
               print("    Table ", i[], ":  (", c[]{0}, ", ", c[]{1},") ... ( ", c[]{2}, ", ", c[]{3},") " );
               echo ("    ('", [test:c[]{0},c[]{1}],"', ...)" );
       }
