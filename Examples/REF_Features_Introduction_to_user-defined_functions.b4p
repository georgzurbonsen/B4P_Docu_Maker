#!/usr/local/bin/b4p
  define function( hypotenuse, { { x, numeral }, { y, numeral } } )
  {
      return( sqrt( x[]*x[] + y[]*y[] ) );
  }

  define procedure( say hello ) { echo("Hello !"); }

  say hello;
  echo( hypotenuse(3,4) );  //  5
  echo( hypotenuse(5,12) ); // 12
  echo( hypotenuse(8,15) ); // 17
  echo( hypotenuse(1,1) );  //  1.41...   
