#!/usr/local/bin/b4p
  define function( diag, { { value 1, numeral }, { value 2, numeral } } )
  {
      return( sqrt( value 1[] * value 1[] + value 2[] * value 2[] ) );
  }

  for all parameters( { diag, sum, product }, function name[]  )
  {
      echo( "Call ", function name[], "(3,4): ", call( function name[], { 3, 4 } ) );
  }
