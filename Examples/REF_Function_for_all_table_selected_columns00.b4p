      table initialize( animals,
          {{ Name, Class, Number of Legs},
           { Dog, Mammal, 4 }, { Snake, Reptile, 0 }, { Human, Mammal, 2 }} );

      // Choose all columns with headers containing 'a'. The list results for row 2.
      for all table selected columns( animals, ('*a*'), 2, column[], header[], i[] )
      {
          echo( i[],": Column ", column[], " (", header[], ") = ", [column[]] );
      }

