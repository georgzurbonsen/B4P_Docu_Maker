#!/usr/local/bin/b4p
table initialize( t, { { 1, 2, 3 }, { 4, 5, 6, 6.1, 6.2 }, { 7, 8, 9 }, { 10, 11, 12 } } );

echo("Original table:");
table list(t);

table rotate left(t, l);
echo("Rotated counterclockwise:");
table list(l);

table rotate right(t);
echo("Orig. table rotated clockwise:");
table list(t);
