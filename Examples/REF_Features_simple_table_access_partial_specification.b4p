#!/usr/local/bin/b4p
  table load( table, "Cities.csv");
  with table( table, 5 ) // Row 5
  {
      echo( [ 1 ] );
      echo( [ -7 ], new line );

      echo( [ City ], new line );

      echo( [ City, San Francisco], new line ); // Exception to rule: Returns row number

      echo( [ City, San Francisco, Famous attraction] );
      echo( [ 1, row(), 4] );
      echo( [ -7, row(), -4] );
  }   
