#!/usr/local/bin/b4p
       a[] = 10;
       b[] = date( "2020-05-01 15:00:00" );
       c[] = { { 1, 2 }, { 3, 4 }, 5 };

       a[] ++ ++ ++ -- ++;        // Becomes 13, even if code appears less meaningful
       b[] ++(3.75) ++;           // 4 days and 18 hours later
